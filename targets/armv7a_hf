#
# armv7a_hf: ARM hardfloat v7 (FPU vfpv3-d16).
#

kernel_arch=arm
libc_arch=arm

multilib_options="--disable-multilib --with-multilib-list="
gcc_options="--with-arch=armv7-a --with-float=hard --with-fpu=vfpv3-d16"

target=arm-${VENDOR}linux-musleabihf

# Define architecture for the package manager
package_arch=armhf
