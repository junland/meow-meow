#
# i586: Intel Pentium MMX optimized for Pentium Pro+ processors.
#

kernel_arch=x86
libc_arch=i386

libSuffix=32

multilib_options="--with-multilib-list=m32"
gcc_options="--with-arch=pentium-mmx --with-tune=pentiumpro --enable-cld"

target=i586-${VENDOR}linux-musl

# Define architecture for the package manager
package_arch=i586
